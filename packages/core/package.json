{
  "name": "@remix-storage/core",
  "version": "0.1.0",
  "description": "File storage solution for Remix with Prisma as ORM. Handle multiple file storage providers like Disk or S3",
  "author": "Andrés Gutiérrez",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "shared-scripts lint src/**/*.{ts,tsx}",
    "build": "tsup ./src/index.ts --dts --format cjs",
    "build:watch": "pnpm run build -- --watch"
  },
  "dependencies": {
    "@tools/shared-scripts": "workspace:*",
    "@tools/eslint-config": "workspace:*",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "tsup": "^5.11.13",
    "typescript": "^4.3.5"
  }
}
