{
  "name": "@remix-storage/bodyparser",
  "version": "0.0.1",
  "private": true,
  "description": "Parse request body on Remix apps allowing a Zod schema for validations. Handling also files fields",
  "keywords": ["files-uploads", "zod", "remix", "web"],
  "license": "MIT",
  "type": "commonjs",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "files": ["/dist"],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && pnpm rollup -c",
    "lint": "prettier -c src/** && eslint src/**/*.ts",
    "lint:fix": "prettier -w src/** && eslint src/**/*.ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@web-std/file": "^3.0.2",
    "busboy": "^1.5.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.34",
    "p-queue": "^7.2.0",
    "slugify": "^1.6.5",
    "tiny-invariant": "^1.2.0",
    "zod": "3.14.2"
  },
  "devDependencies": {
    "@remix-run/express": "^1.3.1",
    "@remix-run/node": "^1.2.3",
    "@types/busboy": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.180",
    "@types/mime-types": "^2.1.1",
    "@types/supertest": "^2.0.11",
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "supertest": "^6.2.2",
    "supertest-fetch": "^1.5.0",
    "vite": "^2.8.4"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": ["darwin", "linux", "win32"]
}
