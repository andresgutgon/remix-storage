    {
      "packageName": "@remix-storage/local",
      "projectFolder": "packages/local",
      "reviewCategory": "packages"
    },
    {
      "packageName": "@tools/eslint-config",
      "projectFolder": "tools/eslint-config",
      "reviewCategory": "tools"
    },
    {
      "packageName": "@tools/shared-scripts",
      "projectFolder": "tools/shared-scripts",
      "reviewCategory": "tools"
    },
    {
      "packageName": "@examples/basic",
      "projectFolder": "examples/basic",
      "reviewCategory": "examples"
    }
  ]
